#!/bin/bash

source bin/setup

bucket_name=`bin/outputs $environment S3BucketName`
file_path=`dirname $(realpath $1)`/hello_world.html

aws s3api put-object \
  --key hello_world.html \
  --bucket $bucket_name \
  --body $file_path \
  --profile $profile \
  --region $region \
  --content-type text/html
